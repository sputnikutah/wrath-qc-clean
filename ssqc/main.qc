void (entity client, string s) centerprint =
{
	float msg_type = MSG_ALL;

	if (client != world)
		msg_type = MSG_ONE;

	msg_entity = client;

	WriteByte	(msg_type, SVC_TEMPENTITY);
	WriteByte	(msg_type, TEMPENT::CENTERPRINT);
	WriteString	(msg_type, s);
}

entity lastspawn;
.float episode;
float deathmatch;	//R00k: moved here from radial.qc

void() worldspawn =
{
	lastspawn = world;

	deathmatch = (cvar("deathmatch"));	//R00k: populate the deathmatch variable.

//	if (!(deathmatch))					//R00k: TODO: reorder this to sandbox deathmatch requirements only.
//	{
		skill = cvar("skill");

		if (!(world.episode))
		{
			switch (self.model)
			{
				case "maps/intro.bsp":
				case "maps/hub1.bsp":
				case "maps/e1m1.bsp":
				case "maps/e1m2.bsp":
				case "maps/e1m3.bsp":
				case "maps/e1m4.bsp":
				case "maps/e1m5.bsp":
				case "maps/boss1.bsp":
					world.episode = 1;
				break;

				case "maps/hub2.bsp":
				case "maps/e2m1.bsp":
				case "maps/e2m2.bsp":
				case "maps/e2m3.bsp":
				case "maps/e2m4.bsp":
				case "maps/e2m5.bsp":
				case "maps/boss2.bsp":
				case "maps/urns2.bsp":
					world.episode = 2;
				break;

				case "maps/hub3.bsp":
				case "maps/e3m1.bsp":
				case "maps/e3m2.bsp":
				case "maps/e3m3.bsp":
				case "maps/e3m4.bsp":
				case "maps/e3m5.bsp":
				case "maps/boss3.bsp":
					world.episode = 3;
				break;
			}
		}

		if (!(world.episode))
			world.episode = 1;

		// Reki (February 14 2024): keep track of our up-to-date save
		// compatibility versions, we'll need to fix stuff if this changes
		map_save_version = world.map_version;
		prog_save_version = PROG_SAVE_VERSION;
		world.prog_version = PROG_SAVE_VERSION;
		//

		//Precache();
		if (!(deathmatch))
		{
			check_enemies_frik();
			check_coffer();
			check_attar();
			check_shockwave();
		}
		check_itemflash();
		check_itemsigil_flash();//used?
		check_itemammount();
		check_itemsize();
		check_itemlight();
		check_maxammo();

		float noqcprecaches = cvar("cl_noqcprecache");

		if (noqcprecaches != 1)
			precaches();

		CSQC_AddStats();

		//	check_tether();
		globalfake_illumination = 1;
//	}//(!deathmatch)

	//
	// Setup light animation tables. 'a' is total darkness, 'z' is maxbright.
	//
	// 0 normal
	lightstyle(0, "m");

	// 1 FLICKER (first variety)
	lightstyle(1, "mmnmmommommnonmmonqnmmo");

	// 2 SLOW STRONG PULSE
	lightstyle(2, "abcdefghijklmnopqrstuvwxyzyxwvutsrqponmlkjihgfedcba");

	// 3 CANDLE (first variety)
	lightstyle(3, "mmmmmaaaaammmmmaaaaaabcdefgabcdefg");

	// 4 FAST STROBE
	lightstyle(4, "mamamamamama");

	// 5 GENTLE PULSE 1
	lightstyle(5, "jklmnopqrstuvwxyzyxwvutsrqponmlkj");

	// 6 FLICKER (second variety)
	lightstyle(6, "nmonqnmomnmomomno");

	// 7 CANDLE (second variety)
	lightstyle(7, "mmmaaaabcdefgmmmmaaaammmaamm");

	// 8 CANDLE (third variety)
	lightstyle(8, "mmmaaammmaaammmabcdefaaaammmmabcdefmmmaaaa");

	// 9 SLOW STROBE (fourth variety)
	lightstyle(9, "aaaaaaaazzzzzzzz");

	// 10 FLUORESCENT FLICKER
	lightstyle(10, "mmamammmmammamamaaamammma");

	// 11 SLOW PULSE NOT FADE TO BLACK
	lightstyle(11, "abcdefghijklmnopqrrqponmlkjihgfedcba");

	// 12 LIGHTNING
	lightstyle(12, "a");

	lightstyle(13, "a");

	lightstyle(14, "a");

	lightstyle(16, "a");

	lightstyle(17, "mmnmmommommnonmmonqnmmo");

	lightstyle(18, "mmnmmommommnonmmonqnmmo");

	lightstyle(19, "mmnmmommommnonmmonqnmmo");


	// styles 32-62 are assigned by the light program for switchable lights

	// 63 testing
	lightstyle(63, "a");
};

